<view class="{{(('_div')+' '+((error)?'has-error':''))}}" id="input"><input type="text" data-event-opts="{{[['change',[['toCheck',['$0','$1'],['pattern','title']]]],['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" onChange="__e" onInput="__e"/><view hidden="{{!(error)}}" class="_small"><view hidden="{{!(requiredError)}}" class="error-message _div">请输入电话号码</view><view hidden="{{!(patternError)}}" class="error-message _div"></view><view hidden="{{!(maxlengthError)}}" class="error-message _div">{{"电话号码长度最大为"+maxlength+"位"}}</view><view hidden="{{!(minlengthError)}}" class="error-message _div">{{"电话号码长度最小为"+minlength+"位"}}</view><view hidden="{{!(existError)}}" class="error-message _div">电话号码不能相同</view></view></view>